{
    "name": "learntowin/general-bundle",
    "type": "symfony-bundle",
    "description": "Learn To Win Bundle for general shared code",
    "keywords": ["symfony", "bundle", "general"],
    "homepage": "https://www.learntowin.com",
    "license": "proprietary",
    "version": "0.2.5",
    "require": {
        "php": ">=8.2",
        "doctrine/dbal": "^3.6",
        "symfony/messenger": "^6.3",
        "symfony/amqp-messenger": "^6.3",
        "symfony/serializer": "^6.3",
        "doctrine/orm": "^2.16",
        "symfony/monolog-bundle": "^3.8",
        "symfony/uid": "^6.3",
        "api-platform/core": "3.1.*",
        "doctrine/doctrine-bundle": "^2.11"
    },
    "require-dev": {
        "symfony/dependency-injection": "^6.3",
        "symfony/config": "^6.3",
        "symfony/phpunit-bridge": ">= 6.1",
        "symfony/http-kernel": "^6.3",
        "symfony/yaml": "^6.3",
        "friendsofphp/php-cs-fixer": "^3.35",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-symfony": "^1.3",
        "phpstan/extension-installer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "LearnToWin\\GeneralBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LearnToWin\\GeneralBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
